{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id":"https://raw.githubusercontent.com/OHDSIBr/ATLAS-JSON-Schema/2.7.4/occurrence.json",
  "type":"object",
  "required":["Type","Count","IsDistinct"],
  "format": "grid",
  "options": {
    "no_additional_properties":true,
    "disable_collapse":true,
    "remove_empty_properties":true,
    "disable_properties":true,
    "disable_edit_json":true
  },
  "properties":{
    "Type":{"options":{"grid_columns": 3},"title":"with","$ref":"occurrence_type_options.json"},
    "Count":{"options":{"grid_columns": 2},"title": " ","$ref":"occurrence_count_options.json"},
    "IsDistinct":{
        "type": "boolean",
        "title": " ",
        "description": " occurrences of criteria: ",
        "default":false,
        "options":{"grid_break": true,"grid_columns": 3,"enum_titles":["using distinct","using all"]}
    }
  }
}