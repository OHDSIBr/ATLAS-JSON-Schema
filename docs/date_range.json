{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type":"object",
  "format":"grid",
  "required": ["Op","Value"],
  "options": {
    "grid_columns":8,
    "disable_collapse":true,
    "disable_properties":true,
    "disable_edit_json":true,
    "no_additional_properties":true,
    "remove_empty_properties": true,
    "keep_oneof_values": false
  },
  "patternProperties": {
    "^Op$": { "type": "string" },
    "^Value$": { "type": "string" },
    "^Extent$": { "type": "string" }
  },
  "additionalProperties": false,
  "oneOf": [
                { "title":"Before","$ref": "core/WithoutExtentBefore.json"},
                { "title":"On or Before","$ref": "core/WithoutExtentOnOrBefore.json"},
                { "title":"On","$ref": "core/WithoutExtentOn.json"},
                { "title":"After","$ref": "core/WithoutExtentAfter.json"},
                { "title":"On or After","$ref": "core/WithoutExtentOnOrAfter.json"},
                { "title":"Between","$ref": "core/WithExtentBetween.json"},
                { "title":"Not Between","$ref": "core/WithExtentNotBetween.json"}
            ]
}