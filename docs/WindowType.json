{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "$id": "WindowType",
  "options":{"disable_edit_json":true,"disable_properties":true},
  "defaultProperties": [
    "Start",
    "End"
  ],
  "required": [
    "Start",
    "End"
  ],
  "properties":{
    "Start":{
    "title":"Start",
    "$ref":"windowLimit.json"
    },
    "End":{
    "type": "object",
    "options":{"disable_edit_json":true,"disable_properties":true},
    "title":"End",
    "properties":{
      "Days":{"$ref":"core/window_day_options.json"},
      "Coeff":{"$ref":"core/window_coeff_options.json"},
      "UseEventEnd":{"format":"checkbox","type":"boolean"}
      }
    }
  }
}