{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type":"object",
    "format":"grid-strict",
    "required": ["Op","Value"],
    "options": {
        "grid_columns":8,
        "disable_collapse":true,
        "disable_properties":true,
        "disable_edit_json":true,
        "no_additional_properties":true,
        "remove_empty_properties": true,
        "keep_oneof_values": false
    },
    "default":{"Op":"gt","Value":0},
    "patternProperties": {
        "^Op$": { "type": "string" },
        "^Value$": { "type": "integer" },
        "^Extent$": { "type": "integer" }
      },
    "additionalProperties": false,
    "oneOf": [
                { "title":"Less Than","$ref": "core/WithoutExtentLessThan.json"},
                { "title":"Less or Equal To","$ref": "core/WithoutExtentLessOrEqualTo.json"},
                { "title":"Equal To","$ref": "core/WithoutExtentEqualTo.json"},
                { "title":"Greater Than","$ref": "core/WithoutExtentGreaterThan.json"},
                { "title":"Greater or Equal To","$ref": "core/WithoutExtentGreaterOrEqualTo.json"},
                { "title":"Between","$ref": "core/WithExtentBetweenNum.json"},
                { "title":"Not Between","$ref": "core/WithExtentNotBetweenNum.json"}
            ]
}