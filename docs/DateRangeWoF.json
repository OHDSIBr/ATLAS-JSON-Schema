{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type":"object",
  "format":"grid-strict",
  "required": ["Op","Value"],
  "options": {
    "grid_columns":8,
    "keep_oneof_values": false
  },
  "patternProperties": {
    "^Op$": { "type": "string" },
    "^Value$": { "type": "string" },
    "^Extent$": { "type": "string" }
  },
  "additionalProperties": false,
  "oneOf": [
              { "title":"Before","$ref": "core/WithoutExtentBeforeWoF.json"},
              { "title":"On or Before","$ref": "core/WithoutExtentOnOrBeforeWoF.json"},
              { "title":"On","$ref": "core/WithoutExtentOnWoF.json"},
              { "title":"After","$ref": "core/WithoutExtentAfterWoF.json"},
              { "title":"On or After","$ref": "core/WithoutExtentOnOrAfterWoF.json"},
              { "title":"Between","$ref": "core/WithExtentBetweenWoF.json"},
              { "title":"Not Between","$ref": "core/WithExtentNotBetweenWoF.json"}
            ]
}